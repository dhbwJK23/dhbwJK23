#Amaount
  transactions =[
    {'type': 'purchase', 'amount': 50, 'date' : '01-02-23'},
    {'type': 'purchase', 'amount': 10, 'date' : '02-02-23'},
    {'type': 'sale', 'amount': 100, 'date' : '01-02-23'}
]

def list_of(my_key):
    amount_values = [transaction[my_key] for transaction in transactions]
    return amount_values
print(list_of('amount'))

